    e: 4 >= 2
    f: 2 >= 2
  }

  falses: {
    a: 4 < 2
    b: 2 > 4
    c: 2 >= 4
    d: 3 <= 2
    e: 4 <= 2
    f: 1 >= 2
  }

  values: {
    four: 2 + 2
    also-four: 2 * 2
