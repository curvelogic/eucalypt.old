#!/usr/bin/env eu -B
# -*- mode: conf -*-


(l + r): __ADD(l, r)
(l - r): __SUB(l, r)
(l * r): __MUL(l, r)
(l / r): __DIV(l, r)

(l > r): __GT(l, r)
(l < r): __LT(l, r)
(l >= r): __GTE(l, r)
(l <= r): __LTE(l, r)

