#!/usr/bin/env eu -B
# -*- mode: conf -*-

# alias the ugly builtins
t:	 __TRUE
f:	 __FALSE
or:	 __OR
and:	 __AND
eq:	 __EQ
not:	 __NOT


trues: {
  a: eq(or(f, and(t, t)), t)
  b: eq(f, not(t))
  c: or(t, t)
  d: or(or(or(or(t, t), t), t), f)
}

falses: {
  a: not(eq(and(f, t), and(t, f)))
  b: or(f, f)
  c: and(t, f)
}
